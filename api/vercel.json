{
  "version": 2,
  "builds": [
    {
      "src": "main.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/main"
    }
  ],
  "env": {
    "PORT": "5000" 
  },
  "functions": {
    "api/main.go": {
      "runtime": "@vercel/go"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/main"
    }
  ]
}